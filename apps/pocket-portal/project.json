{
  "name": "pocket-portal",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/pocket-portal/src",
  "prefix": "app",
  "generators": {},
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "options": {
        "outputPath": "dist/pocket-portal",
        "index": "apps/pocket-portal/src/index.html",
        "browser": "apps/pocket-portal/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/pocket-portal/tsconfig.app.json",
        "assets": [
          "apps/pocket-portal/src/favicon.ico",
          "apps/pocket-portal/src/assets"
        ],
        "styles": ["apps/pocket-portal/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "pocket-portal:build:production"
        },
        "development": {
          "buildTarget": "pocket-portal:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/pocket-portal/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "pocket-portal:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": ["zone.js", "zone.js/testing"],
        "tsConfig": "apps/pocket-portal/tsconfig.spec.json",
        "assets": [
          "apps/pocket-portal/src/favicon.ico",
          "apps/pocket-portal/src/assets"
        ],
        "styles": ["apps/pocket-portal/src/styles.css"],
        "scripts": []
      }
    }
  }
}
